        static boolean substring(String str)
        {
            int n=str.length();
           char ch[]=str.toCharArray();
            for(int k=0;k<n;k++)
         {
         System.out.println("Enter substring of" + "str1");
        for(int i=0;i<n;i++)
        {
          System.out.print("");
          System.out.println("Enter substring of" +"str2");
          for(int j=0;j<n;j++)
        {
         System.out.print("");
